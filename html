       <!-- /Comentários --> 
       <script>
folha.comments = {
	// caixa de comentários
	box: "#articleComments" ,

	// coleção de comentários
	cache: [] ,
	cache_name: {user:{}} ,

	// links
	links: [] ,
	
	maintenance: false ,

	domain: "" ,

	get_context: function() {
		var hostname = location.hostname.split( "." )
		if ( hostname[0] == "test" || hostname[0] == "dev" ) {
			return hostname[0] + "." ;
		}

		if ( ! isNaN( hostname[0] ) ) {
			return hostname.slice( 0 , 2 ).join( "." ) + "." ;
		}

		if ( hostname[0] == "staging" ) {
			return "dev." ;
		}

		return "" ;
	} 
} ;

folha.evaluations = {
	evaluations_box_limit: 4 ,
	users: {} ,
	box: "#comments-overbox" ,

	init : function() {
		var that = folha.evaluations ,
		html = [] ;

		html.push( '<div id="comments-overbox">' ,
		'<p class="evaluations_box_header">Avaliar comentário como <span class="evaluations_type"></span>.</p>' ,
		'<a href="#" class="evaluations_confirm_btn">Confirmar</a>' ,
		'<div class="evaluators"></div><span class="arrow"></span><span class="arrow arrow-last"></span>' ,
		'</div><div id="comments-lightbox"><h1 class="toptitle"></h1>' ,
		'<ul class="evaluators_list"><li></li></ul></div>' ) ;
		
		$( function() {
			$( "body" ).append( html.join( "" ) ) ;
	
			$( "#comments .to-thumb a" ).each( function() {
				$( this ).bind( "click" , function( e ) {
					e.preventDefault() ;
					if ( $( this ).hasClass( "activated" ) ) {
						that.hide_box() ;
					}
					else {
						$( "#comments .to-thumb a" ).removeClass( "activated" ) ;
						that.show_box( this ) ;
						$( this ).addClass( "activated" ) ;
					}
				} ) ;
			} ) ;
		} ) ;		
		
	} ,

	hide_box: function() {
		var that = folha.evaluations ;
		$( that.box ).hide() ;
		$( "#comments .to-thumb a" ).removeClass( "activated" ) ;
	} ,

	show_box: function( elem ) {
		var position = { top:"" , left:"" } ,
		comment_id ,
		type = "up" ,
		evaluated ,
		that = folha.evaluations ;

		comment_id = $( elem ).data( "comment-id" ) ;
		evaluated = $( elem ).parents( ".comment_li:first" ).hasClass( "evaluated" ) ;
		position.top = $( elem ).position().top ;

		if ( $( "#comments" ).position().left == 0 ) {
			//herança de position relative F5
			position.left = $( elem ).position().left + $( "body" ).children().offset().left ;
		}
		else if( $( "body" ).css( "position" ) == "relative" ) {
			//herança body position relative Agora
			position.left = $( elem ).position().left - $( "body" ).children( "div" ).position().left ;
		}
		else {
			//sem herança de posição
			position.left = $( elem ).position().left ;
		}

		//fecha ao tirar o mouse da caixa, obs a seta é parcialmente parte da caixa
		$( that.box ).unbind().bind( "mouseleave" , function() {
			that.hide_box() ;
		} ) ;

		//ao clicar sobre a seta que esta parcialmente por cima do botao tambem fecha a caixa
		$( ".arrow-last" , that.box ).unbind().bind( "click" , function() {
			that.hide_box() ;
		} ) ;

		//limpando
		$( ".evaluations_computing_msgs" , that.box ).remove() ;
		$( ".evaluations_confirm_btn" , that.box ).show() ;

		//frase cabeçalho
		if ( $( elem ).hasClass( "bad" ) ) {
			$( ".evaluations_type" , that.box ).html( "negativo" ) ;
			$( ".evaluations_box_header" , that.box ).addClass( "bad" ).removeClass( "good" ) ;
			type = "down" ;
		}
		else {
			$( ".evaluations_type" , that.box ).html( "positivo" ) ;
			$( ".evaluations_box_header" , that.box ).addClass( "good" ).removeClass( "bad" ) ;
		}

		//mostra o botão confirmar ou nao se o usuário já avaliou
		if ( ! evaluated ) {
			$( ".evaluations_confirm_btn" , that.box ).show()
			//setando o click do botao confirmar
			$( ".evaluations_confirm_btn" , that.box ).unbind( "click" ).bind( "click" , function( e ) {
				e.preventDefault() ;
				that.compute_vote( elem , comment_id , type ) ;
			} ) ;
		}
		else {
			$( ".evaluations_confirm_btn" , that.box ).hide() ;
			$( ".evaluations_box_header" , that.box ).after( "<p class=\"evaluations_computing_msgs\">Você já avaliou este comentário anteriormente. Só é permitido avaliar uma única vez.</p>" ) ;
		}

		//quem avaliou
		$( ".evaluators" , that.box ).html( "" ) ;
		if ( that.users[comment_id] !== undefined ) {
			that.show_initial_users( comment_id , type ) ;
		}
		else {
			that.get_initial_users( comment_id , type ) ;
		}

		//mostrando o box
		$( that.box ).css( {top: position.top , left: position.left } ).show() ;

	} ,

	get_initial_users: function( comment_id , type ) {
		var that = folha.evaluations ,
		callback = "get_initial_users" ;
		
		$.ajax( {
			url: folha.comments.domain + "/avaliacao/" + comment_id + ".jsonp" ,
			type: "GET" ,
			dataType: "jsonp" ,
			crossDomain: true ,
			jsonpCallback: callback ,
			beforeSend: function() {
				$( ".evaluators" , that.box ).html( "<p class=\"evaluators_loading\">Carregando...</p>" ) ;
			} ,
			success: function( data ) {
				that.users[comment_id] = data ;
				if ( that.users[comment_id] !== undefined && that.users[comment_id][type] !== undefined ) {
					that.show_initial_users( comment_id , type ) ;
				}
				else {
					$( ".evaluators" , that.box ).html( "" ) ;
				}
			}
		} ) ;
	} ,

	show_initial_users: function( comment_id , type ) {
		var lightbox = "#comments-lightbox" ,
		html = [ '<p>Também avaliaram:</p><ul class="evaluators_list">' ] ,
		html_lightbox = [] ,
		total = 0 ,
		i = 0 ,
		that = folha.evaluations ,
		ancor ;

		if ( that.users[comment_id][type] !== undefined ) {
			total = that.users[comment_id][type].length ;
		}

		for ( i = 0 ; i < total ; i++ ) {
			//drop box
			if ( i < that.evaluations_box_limit ) {
				html.push ( '<li><a href="' ,
				that.users[comment_id][type][i].user_url ,
				'">' ,
				that.users[comment_id][type][i].user_name ,
				'</a></li>' ) ;
			}
			//light box
			html_lightbox.push ( '<li><a href="' ,
			that.users[comment_id][type][i].user_url ,
			'">' ,
			that.users[comment_id][type][i].user_name ,
			'</a></li>' ) ;
		}

		html.push( '</ul>' ) ;

		if ( total > 0 ) {
			$( ".evaluators" , that.box ).html( html.join( "" ) ) ;
			$( ".evaluators" , that.box ).show() ;

			if ( total > that.evaluations_box_limit ) {
				//cria link para lightbox
				ancor = $( "<a/>" ) ;
				$( ancor )
				.attr( "href" , "#" )
				.html( "e +" + ( total - that.evaluations_box_limit ) + " outro" + ( ( total - that.evaluations_box_limit ) > 1 ? "s" : "" ) )
				.unbind()
				.bind( "click" , function( e ) {
					e.preventDefault() ;
					$.fancybox.open( lightbox ,
						{
							//not set margin, problem site agora
							width : 380 ,
							height : 300 ,
							autoSize : false ,
							padding : 0 ,
							wrapCSS : "fancybox-comments" ,
							tpl : { closeBtn : '<a title="Fechar" class="fancybox-item fancybox-close" href="javascript:;">X</a>' }
						}
					) ;
				} ) ;
				$( that.box ).find( "ul.evaluators_list" ).append( $( "<li/>" ).prepend( ancor ) ) ;
			}
			$( "h1.toptitle" , lightbox ).html( "Avaliações " +  ( type === "up" ? "positivas" : "negativas" ) + " (" + total + ")" ) ;
			$( ".evaluators_list" , lightbox ).html( html_lightbox.join( "" ) ) ;

		}
	} ,
	compute_vote: function( elem , comment_id , type ) {
		var that = folha.evaluations ,
		callback = "compute_vote" ,
		box_header ;

		box_header = $( ".evaluations_box_header" , that.box ) ;
		$( ".evaluations_confirm_btn" , that.box ).hide() ;
		$.ajax( {
			url: folha.comments.domain + "/avaliacao/votar/" + comment_id + ".jsonp" ,
			type: "GET" ,
			dataType: "jsonp" ,
			crossDomain: true ,
			jsonpCallback: callback ,
			data: {
				type: type ,
				done_url: document.URL
			} ,
			beforeSend: function() {
				box_header.after( "<p class=\"evaluations_computing_msgs\">Enviando...</p>" ) ;
			} ,
			success: function( data ) {
				$( ".evaluations_computing_msgs" , that.box ).html( data.message ) ;
				
				if ( data.status != "error" ) {
					$( elem ).parents( ".comment_li:first" ).addClass( "evaluated" ) ;
				} 

			} ,
			error: function() {
				box_header.after( "<p class=\"evaluations_computing_msgs\">Erro ao enviar</p>" ) ;
			}
		} ) ;
	}
};

folha.evaluations.evaluations_box_limit = 4 ;
folha.evaluations.init() ;
</script> 
      </form> 
     </section> 
     <!-- /main --> 
    </div> 
    <!-- /span12 --> 
