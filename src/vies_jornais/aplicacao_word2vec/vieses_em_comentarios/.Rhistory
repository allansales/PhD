organize_input = function(x, y){
X = x %>% t() %>% as_data_frame()
Y = y %>% t() %>% as_data_frame()
table = list(Xi = X, Yi = Y)
}
cosine_metrics = create_cosine_metrics(x, y, a, b, modelo)
