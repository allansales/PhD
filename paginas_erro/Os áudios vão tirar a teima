  <script type="text/javascript">
       /* Mobile */
       if (window.innerWidth < 436){
           var contentNoticia = $(".n--noticia[data-guid='wpn_347_240']");
           var boxContentNoticia = contentNoticia.find(".col-center .box.area-select");
           var limiteContinuarLendo = contentNoticia.find(".limite-continuar-lendo");
           console.log("containers noticia", contentNoticia, boxContentNoticia, limiteContinuarLendo);
           if (isArray(limiteContinuarLendo) && limiteContinuarLendo.size() > 0){
               
               //tem o limite de tamanho da notícia
               var continuarLendoHeight = limiteContinuarLendo.offset().top - boxContentNoticia.offset().top;
               boxContentNoticia.css({height: continuarLendoHeight + 'px'});
               $('.continuar-lendo').addClass('block');
           } else{
                console.log("containers noticia 2");
               //não tem o limite de tamanho da notícia
               contentNoticia.addClass('block-elements');
           }
       }
