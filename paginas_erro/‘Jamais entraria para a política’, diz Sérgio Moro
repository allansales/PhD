    var estadao_info = {"produto": "Estad\u00e3o", "editoria": "Politica", "subeditoria": "Geral", "id_documento": "wpn_41_60461", "tags": "operação Lava Jato;Sérgio Moro", "tipo_midia": "Not\u00edcias", "autor": "Fausto Macedo e Ricardo Brandt, enviados especiais a Curitiba"};
